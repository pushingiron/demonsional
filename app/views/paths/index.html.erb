<table class="table">
  <thead>
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th>Object</th>
    <th>Action</th>
    <th>Data</th>
  </tr>
  </thead>

  <tbody>
  <% @paths.each do |paths| %>
    <tr>
      <td><%= paths.created_at %></td>
      <td><%= paths.description %></td>
      <td><%= paths.object %></td>
      <td><%= paths.action %></td>
      <% if paths.data.blank? %>
      <td></td>
      <% else %>
        <td><%= link_to "View XML", path_view_xml_path(paths.id), target: :_blank %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>