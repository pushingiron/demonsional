<tbody>
<tr>
  <td>
    <%= contract.contract_name %>
  </td>
  <td>
    <%= contract.rate_table %>
  </td>
  <td>
    <%= contract.effective_date %>
  </td>
  <td>
    <%= contract.expiration_date %>
  </td>
  <td>
    <%= contract.contract_type %>
  </td>

  <td>
    <%= link_to 'Edit', edit_contract_path(contract) %>
  </td>
  <td>
    <%= link_to 'Copy', copy_contracts_path(contract) %>
  </td>
  <td>
    <%= button_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
</tbody>